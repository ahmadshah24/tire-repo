<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="vender_list">
      <field name="name">vender.list</field>
      <field name="model">vender.vender</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="address"/>
          <field name="phone"/>
           <field name="active"/>
        </tree>
      </field>
    </record>

<!-- form _________________________________ -->

    <record model="ir.ui.view" id="vender_form">
      <field name="name">vender.form</field>
      <field name="model">vender.vender</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
                <group>
                    <field name="name" required="1"/>
                    <field name="address"/>
                    <field name="phone"/>
                     <field name="active"/>
                </group>
            </group>
              <group>
                     <field name="des"/>
              </group>
        </sheet>
        </form>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="vender_action_window">
      <field name="name">Sobek Vender</field>
      <field name="res_model">vender.vender</field>
      <field name="view_mode">tree,form</field>
    </record>

  </data>
</odoo>