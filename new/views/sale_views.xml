<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="atlas_sale_list">
      <field name="name">atlas.sale.list</field>
      <field name="model">atlas.sale</field>
      <field name="arch" type="xml">
        <tree>
          <field name="no"/>
          <field name="billNo"/>
          <field name="costumer_id"/>
          <field name="date"/>
          <field name="total" sum="a"/>
           <field name="active"/>
        </tree>
      </field>
    </record>

<!-- form _________________________________ -->

    <record model="ir.ui.view" id="atlas_sale_form">
      <field name="name">atlas.sale.form</field>
      <field name="model">atlas.sale</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
                <group>
                    <field name="no"/>
                    <field name="billNo"/>
                    <field name="costumer_id"/>
                </group>
                <group>
                    <field name="date"/>
                    <field name="total"/>
                     <field name="active"/>
                </group>
            </group>
            <notebook>
              <page string="Return Information">
				<group>
						<field name="return_id">
					 	<tree editable="top">
							<field name="no"/>
							<field name="billNo"/>
							<field name="Cname"/>
							<field name="date"/>
							<field name="total"/>
					 	</tree>
						</field>
				</group>
              </page>
            </notebook>
        </sheet>
        </form>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="sale_action_window">
      <field name="name">Sobek sale</field>
      <field name="res_model">atlas.sale</field>
      <field name="view_mode">tree,form</field>
    </record>


     <record model="ir.actions.act_window" id="return_action_window">
      <field name="name">Sobek return</field>
      <field name="res_model">return.return</field>
      <field name="view_mode">tree,form</field>
    </record>



	<record model="ir.actions.act_window" id="rasidat_action_window">
      <field name="name">Sobek Rasidat</field>
      <field name="res_model">rasidat.rasidat</field>
      <field name="view_mode">tree,form</field>
    </record>


  <record model="ir.actions.act_window" id="costumer_action_window">
        <field name="name">Sobek Sales to Costumers</field>
        <field name="res_model">costumer.costumer</field>
        <field name="view_mode">tree,form</field>
      </record>


      

    <record model="ir.actions.act_window" id="crasidat_action_window">
      <field name="name">Sobek Rasidat from costumers </field>
      <field name="res_model">crasidat.crasidat</field>
      <field name="view_mode">tree,form</field>
    </record>



    <record model="ir.actions.act_window" id="take_action_window">
      <field name="name">Sobek Takes From Office</field>
      <field name="res_model">take.take</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="stock_action_window">
      <field name="name">Sobek Stock Tire And Tube</field>
      <field name="res_model">stock.stock</field>
      <field name="view_mode">tree,form</field>
    </record>


     <record model="ir.actions.act_window" id="costumerinfo_action_window">
      <field name="name">Sobek Costumers Information</field>
      <field name="res_model">costumerinfo.costumerinfo</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="atlas_product_action_window">
      <field name="name">Sobek Atlas Product</field>
      <field name="res_model">atlas.product</field>
      <field name="view_mode">tree,form</field>
    </record>



    <record model="ir.actions.act_window" id="atlas_sale_line_action_window">
      <field name="name">Sobek Atlas sale Line</field>
      <field name="res_model">atlas.sale.line</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="atlas_purchase_action_window">
      <field name="name">Sobek purchase</field>
      <field name="res_model">atlas.purchase</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="atlas_purchase_line_action_window">
      <field name="name">Sobek Atlas purchase Line</field>
      <field name="res_model">atlas.purchase.line</field>
      <field name="view_mode">tree,form</field>
    </record>
 
    <record model="ir.actions.act_window" id="vender_action_window">
      <field name="name">Sobek Vender</field>
      <field name="res_model">vender.vender</field>
      <field name="view_mode">tree,form</field>
    </record>
    <!-- Top menu item -->

    <menuitem name="Sobek Tire" id="new.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Sale" id="new.menu_sale_root" parent="new.menu_root"/>
	<menuitem name="Rasidat" id="new.menu_3" parent="new.menu_root" action="rasidat_action_window"/>
    <menuitem name="Costumer" id="new.menu_4" parent="new.menu_root" action="costumer_action_window"/>
    <menuitem name="Costumer Rasidat" id="new.menu_5" parent="new.menu_root" action="crasidat_action_window"/>
    <menuitem name="Takes" id="new.menu_6" parent="new.menu_root" action="take_action_window"/>
    <menuitem name="Stock" id="new.menu_7" parent="new.menu_root" action="stock_action_window"/>
    <menuitem name="Product" id="new.menu_product_root" parent="new.menu_root"/>
    <menuitem name="Purchase" id="new.menu_purchase_root" parent="new.menu_root"/>

    <!-- submenus -->
    <menuitem name="Sale" id="new.menu_sale" parent="new.menu_sale_root" action="sale_action_window"/>
    <menuitem name="Customer" id="new.menu_customer" parent="new.menu_sale_root" action="costumerinfo_action_window"/>
    <menuitem name="Return" id="new.menu_return" parent="new.menu_sale_root" action="return_action_window"/>
    <menuitem name="product" id="new.menu_product" parent="new.menu_product_root" action="atlas_product_action_window"/>
    <menuitem name="Sale line" id="new.menu_sale_line" parent="new.menu_sale_root" action="atlas_sale_line_action_window"/>
    <menuitem name="purchase"  id="new.menu_purchase" parent="new.menu_purchase_root" action="atlas_purchase_action_window"/>
    <menuitem name="purchase line"  id="new.menu_purchase_line" parent="new.menu_purchase_root" action="atlas_purchase_line_action_window"/>
    <menuitem name="Vender"  id="new.menu_vender" parent="new.menu_purchase_root" action="vender_action_window"/>

  </data>
</odoo>