<odoo>
  <data>

    <record model="ir.ui.view" id="atlas_genral_report_wizard_form_view">
        <field name="name">Atlas Genral Report Wizard</field>
        <field name="model">atlas.genral.report.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="start_date" />
                            <field name="end_date" />
                        </group>
                    </group>
                </sheet>
                <footer>
                    <button string="Print" name="print_atlas_general_report" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
     <record model="ir.actions.act_window" id="atlas_genral_report_wizard_action_window">
      <field name="name">Atlas Genral Report</field>
      <field name="res_model">atlas.genral.report.wizard</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="atlas_genral_report_wizard_form_view"/>
      <field name="target">new</field>
    </record>


    <menuitem name="Reports" id="atlas_genral_reports_menu" parent="new.menu_root" sequence="4" />  
    <menuitem name="Atlas Genral Report" id="atlas_genral_report_menu_1" parent="atlas_genral_reports_menu"
        action="atlas_genral_report_wizard_action_window" />  


  </data>
</odoo>