<odoo>
    <template id="monthly_expense_report">
        <t t-call="web.html_container">
            <t t-foreach="data">
                <div class="page">
                    <h1>This is my first report</h1>
                    <!-- <p><span t-esc="data.date"/></p> -->
                </div>
            </t>
        </t>
    </template>
    <odoo>
    <record model="ir.actions.report" id="action_atlas_genral_report">
        <field name="name">Atlas Genral Report</field>
        <field name="model">atlas.genral.report.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">new.atlas_genral_report</field>
        <field name="report_file">new.atlas_genral_report</field>
        <field name="print_report_name">'Altas Genral Report'</field>
        <field name="binding_model_id" ref="model_atlas_genral_report_wizard"></field>
        <field name="binding_type">report</field>
    </record>
</odoo>
</odoo>
  